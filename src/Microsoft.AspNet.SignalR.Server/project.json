{
    "version": "3.0.0-*",
    "description": "Core server components for ASP.NET SignalR.",
    "dependencies": {
        "Microsoft.AspNet.HttpFeature": { "version": "1.0.0-*", "type": "build" },
        "Microsoft.AspNet.RequestContainer": "1.0.0-*",
        "Microsoft.AspNet.Security.DataProtection": "1.0.0-*",
        "Microsoft.Framework.Logging": "1.0.0-*",
        "Microsoft.Framework.Runtime.Interfaces": { "version": "1.0.0-*", "type": "build" },
        "Newtonsoft.Json": "6.0.6"
    },
    "compilationOptions": {
        "warningsAsErrors": true,
        "allowUnsafe": true
    },
    "resources": "Scripts/**/*.*",
    "frameworks": {
        "aspnet50": { },
        "aspnetcore50": {
            "dependencies": {
                "Microsoft.CSharp": "4.0.0-beta-*",
                "System.Collections.Concurrent": "4.0.10-beta-*",
                "System.Dynamic.Runtime": "4.0.0-beta-*",
                "System.Linq.Expressions": "4.0.0-beta-*",
                "System.Reflection.Emit": "4.0.0-beta-*",
                "System.Reflection.Emit.ILGeneration": "4.0.0-beta-*",
                "System.Reflection.Extensions": "4.0.0-beta-*",
                "System.Reflection.Primitives": "4.0.0-beta-*",
                "System.Security.Cryptography.RandomNumberGenerator": "4.0.0-beta-*",
                "System.Threading.Thread": "4.0.0-beta-*",
                "System.Threading.ThreadPool": "4.0.10-beta-*",
                "System.Threading.Timer": "4.0.0-beta-*"
            }
        }
    }
}
